{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "AngularDBG": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "../../../src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/AngularDBG",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "oidc-client-ts.js",
                "input": "../../dist",
                "output": "."
              },
              {
                "glob": "silent-callback.html",
                "input": "src",
                "output": "./assets"
              },
              {
                "glob": "signin-callback.html",
                "input": "src",
                "output": "./assets"
              },
              {
                "glob": "signout-callback.html",
                "input": "src",
                "output": "./assets"
              }
            ],
            "styles": [],
            "scripts": []
          },
          "configurations": {
            "debug": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "defaultConfiguration": "debug"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "debug": {
              "browserTarget": "AngularDBG:build:debug"
            },
            "production": {
              "browserTarget": "AngularDBG:build:production"
            }
          },
          "defaultConfiguration": "debug"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "AngularDBG:build"
          }
        }
      }
    }
  },
  "defaultProject": "AngularDBG"
}